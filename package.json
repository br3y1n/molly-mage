{
  "name": "molly-mage",
  "version": "1.0.0",
  "description": "this is bot to molly mage - epam challenge",
  "main": "dist/server.js",
  "scripts": {
    "prepare": "husky install",
    "start": "tsc && node dist/server.js",
    "validate": "pretty-quick --staged && yarn depcheck && yarn lint && yarn test",
    "lint": "yarn prettier --write 'src/**/*.ts'",
    "depcheck": "depcheck . --ignores=\"@types/jest\"",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/br3y1n/molly-mage.git"
  },
  "keywords": [
    "bot",
    "epam",
    "mollyMage"
  ],
  "author": "BreYin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/br3y1n/molly-mage/issues"
  },
  "homepage": "https://github.com/br3y1n/molly-mage#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "29.2.3",
    "depcheck": "1.4.3",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "prettier": "2.8.0",
    "pretty-quick": "3.1.3",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3"
  }
}
